<div id="gamecontainer">
   <div id="gamescreen">
      <div id="sky" class="animated">
         <div id="flyarea">
            <div id="ceiling" class="animated"></div>
            <!-- This is the flying and pipe area container -->
            <div id="player" class="bird animated"><div id="login"></div></div>

            <div id="bigscore"></div>

            <div id="splash"></div>

            <div id="scoreboard">
               <div id="medal"></div>
               <div id="currentscore"></div>
               <div id="highscore"></div>
               <div id="replay"><img src="<%= static_path(@conn, "/images/replay.png") %>" alt="replay"></div>
            </div>

            <!-- Pipes go here! -->
         </div>
      </div>
      <div id="land" class="animated"><div id="debug"></div></div>
   </div>
</div>
<div id="footer">
   <a href="http://www.dotgears.com/">original game/concept/art by dong nguyen</a>
   <a href="http://nebezb.com/">recreated by nebez briefkani</a>
   <a href="http://github.com/nebez/floppybird/">view github project</a>
</div>
<div class="boundingbox" id="playerbox"></div>
<div class="boundingbox" id="pipebox"></div>

<script src="<%= static_path(@conn, "/js/app.js") %>"></script>
<script src="<%= static_path(@conn, "/js/vendor.js") %>"></script>
